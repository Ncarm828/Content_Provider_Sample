<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:id="@android:id/secondaryProgress">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270" >
            <shape
                android:innerRadiusRatio="3"
                android:shape="ring"
                android:thicknessRatio="24.0"
                android:useLevel="true" >
                <gradient
                    android:centerColor="#f6fdf8"
                    android:endColor="#f6fdf8"
                    android:startColor="#f6fdf8"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
    <item android:id="@android:id/progress">
        <rotate
            android:fromDegrees="270"
            android:pivotX="50%"
            android:pivotY="50%"
            android:toDegrees="270" >
            <shape
                android:innerRadiusRatio="3"
                android:shape="ring"
                android:thicknessRatio="24.0"
                android:useLevel="true" >
                <gradient
                    android:centerColor="#26ce61"
                    android:endColor="#5fe18d"
                    android:startColor="#5e9672"
                    android:type="sweep" />
            </shape>
        </rotate>
    </item>
</layer-list>