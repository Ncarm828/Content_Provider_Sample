<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="appsandmaps.temple.edu.content_provider" >
    
    <uses-permission android:name="edu.temple.app.provider.permission.READ_PROVIDER"></uses-permission>
    <uses-permission android:name="edu.temple.app.provider.permission.WRITE_PROVIDER"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".MyContentProvider"
            android:authorities="edu.temple.app.provider"
            android:enabled="true"
            android:exported="true">
        </provider>
    </application>

</manifest>
